<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.njit.edu.pojo">

	<class name="Linkinfo" table="t_Linkinfo" >
		
		    <id name="LinkId" column="LinkId">
		    <generator class="identity"/>
		    </id>
		    
			<property name="LinkName" column="LinkName"/>
			
			<property name="LinkURL" column="LinkURL"/>
			
			<property name="Linkjude" column="Linkjude"/>
			
			<property name="UpdateTime" column="UpdateTime"/>
						
	</class>
	
</hibernate-mapping>